{
  "name": "arva-ds",
  "version": "1.0.0-beta-1",
  "description": "Using the latest ES6 standards creating a simple, compact DataSource library that enables plain object interaction in Javascript and automated synchronisation.",
  "main": false,
  "homepage": "https://github.com/bizboard/arva-ds",
  "repository": {
    "type": "git",
    "url": "git://github.com/bizboard/arva-ds.git"
  },
  "scripts": {
    "build": "jspm bundle-sfx src/main.js dist/arva-ds.js",
    "test": "node_modules/.bin/mocha --compilers js:babel/register --timeout 15000"
  },
  "bugs": {
    "url": "https://github.com/bizboard/arva-ds/issues"
  },
  "jspm": {
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "SPSoapAdapter": "github:bizboard/SPSoapAdapter@1.0.0-beta-3",
      "arva-utils": "github:bizboard/arva-utils@1.0.0-beta-3",
      "di": "github:bizboard/di.js@master",
      "eventemitter3": "npm:eventemitter3@^1.1.0",
      "firebase": "github:firebase/firebase-bower@^2.2.0",
      "lodash": "npm:lodash@^4.2.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.6.4",
      "babel-runtime": "npm:babel-runtime@^5.6.4",
      "core-js": "npm:core-js@^0.9.17",
      "traceur": "github:jmcriffey/bower-traceur@^0.0.92",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.90"
    }
  },
  "author": "Tom Clement <tom.clement@bizboard.nl>, Hans van den Akker <hans@bizboard.nl>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.4.0",
    "isparta": "^3.1.0",
    "mocha": "^2.3.3",
    "systemjs": "^0.19.5"
  }
}
